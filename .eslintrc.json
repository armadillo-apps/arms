{
  "plugins": ["cypress", "prettier"],
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "cypress/globals": true
  },
  "extends": [
    "react-app",
    "plugin:cypress/recommended",
    "eslint:recommended",
    "plugin:prettier/recommend"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": 0,
    "prettier/prettier": "error"
  }
}
