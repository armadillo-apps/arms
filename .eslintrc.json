{
  "plugins": ["cypress"],
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "cypress/globals": true
  },
  "extends": ["react-app", "plugin:cypress/recommended", "eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": 0
  }
}
